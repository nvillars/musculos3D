{
  "name": "anatomical-3d-viewer",
  "version": "1.0.0",
  "description": "Interactive 3D anatomical viewer for educational purposes",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "npm run clean && webpack --mode production",
    "build:analyze": "npm run clean && webpack --mode production --env analyze",
    "build:staging": "npm run clean && webpack --mode production --env staging",
    "build:production": "npm run clean && webpack --mode production --env production",
    "build:stats": "npm run clean && webpack --mode production --json > dist/stats.json",
    "server": "node server/proxy-server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "preview": "npm run build && npm run server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:performance": "jest --testPathPattern=performance",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "test:cross-browser": "node tests/cross-browser/run-tests.js",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "audit": "npm audit && npm run lighthouse",
    "clean": "rimraf dist",
    "deploy:staging": "npm run build:staging && npm run deploy:upload:staging",
    "deploy:production": "node scripts/deploy-production.js && npm run deploy:upload:production",
    "deploy:upload:staging": "echo 'Upload to staging server'",
    "deploy:upload:production": "echo 'Upload to production server'",
    "start:production": "NODE_ENV=production node server/production-server.js",
    "start:staging": "NODE_ENV=staging node server/production-server.js",
    "optimize": "npm run build:stats && npm run build:analyze",
    "precommit": "npm run test:ci && npm run build"
  },
  "keywords": [
    "anatomy",
    "3d",
    "medical",
    "education",
    "threejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "three": "^0.160.1",
    "three-stdlib": "^2.36.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@jest/test-sequencer": "^29.7.0",
    "babel-loader": "^9.1.3",
    "canvas": "^2.11.2",
    "chrome-launcher": "^1.1.0",
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "gltf-pipeline": "^4.3.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-performance-testing": "^1.0.0",
    "lighthouse": "^11.3.0",
    "mini-css-extract-plugin": "^2.7.6",
    "obj2gltf": "^3.1.6",
    "playwright": "^1.40.0",
    "puppeteer": "^21.5.0",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.101.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testMatch": [
      "<rootDir>/tests/**/*.test.js",
      "<rootDir>/tests/**/*.spec.js"
    ],
    "moduleNameMapping": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
